<section class="question-body">
  <div class="progression-image" style="background: url('{{imageLevel}}') 0% 50% no-repeat; background-size:contain;">
  </div>
  <div class="container container-960">
    <div class="intro-block">
      <h1>Answer Some Questions</h1>
      <p class="question">{{questionIndex}}</p>
    </div>
    <div class="button-container">
      {{#each currentTree.buttons}}
        <div class="btn btn-primary question-choice" id="{{@index}}" role="button">{{title}}</div>
      {{/each}}
    </div>
  </div>
</section>