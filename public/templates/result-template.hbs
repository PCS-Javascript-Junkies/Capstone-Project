<section class="question-body">
  <div class="progression-image" style="background: url('./img/5.jpg') 0% 50% no-repeat; background-size:contain;">
  </div>
  <div class="container">
    <div class="intro-block">
      <h1>Your Adventure:</h1>
    </div>
    <div class="col-md-6 result-story">
      <p>{{displayResults.attributes.story.[0]}} {{displayResults.attributes.story.[1]}}
      <span class="result-in-story">{{displayResults.attributes.results.[0].name}}</span>
      {{displayResults.attributes.story.[2]}}
      </p>
      <p>
      {{displayResults.attributes.story.[3]}}
      <span class="result-in-story">{{displayResults.attributes.results.[1].name}}</span>
      {{displayResults.attributes.story.[4]}}
      </p>
      <p>
      {{displayResults.attributes.story.[5]}}
      <span class="result-in-story">{{displayResults.attributes.results.[2].name}}</span>
      {{displayResults.attributes.story.[6]}}
      </p>
      <p>
      {{displayResults.attributes.story.[7]}}
      <span class="result-in-story">{{displayResults.attributes.results.[3].name}}</span>
      {{displayResults.attributes.story.[8]}}
      </p>
    </div>
    <div class="col-md-6 result-details">
        {{#each displayResults.attributes.results}}
      <h1>{{name}}</h1>
      <img src={{img}} />
      <p>{{address}}</p>
      {{/each}}
    </div>
  </div>
</section>